<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Info_window">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="shelter"
        app:layout_constraintTop_toBottomOf="@+id/imageViewShelter"
        tools:layout_editor_absoluteX="57dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="camera"
        app:layout_constraintTop_toBottomOf="@+id/imageViewCamera"
        tools:layout_editor_absoluteX="169dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="24hours"
        app:layout_constraintTop_toBottomOf="@+id/imageView24hrs"
        tools:layout_editor_absoluteX="289dp" />


    <ImageView
        android:id="@+id/imageViewStar3"
        android:layout_width="32dp"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:contentDescription="reviewStar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/starall" />


    <ImageView
        android:id="@+id/imageViewStar1"
        android:layout_width="32dp"
        android:layout_height="22dp"
        android:contentDescription="reviewStar"
        app:layout_constraintStart_toEndOf="@+id/imageViewStar3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/starhalf" />


    <ImageView
        android:id="@+id/imageViewStar2"
        android:layout_width="32dp"
        android:layout_height="22dp"
        android:contentDescription="reviewStar"
        app:layout_constraintEnd_toStartOf="@+id/imageViewStar3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/starall" />


    <ImageView
        android:id="@+id/imageViewStar4"
        android:layout_width="32dp"
        android:layout_height="22dp"
        android:contentDescription="reviewStar"
        app:layout_constraintEnd_toStartOf="@+id/imageViewStar2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/starall" />


    <ImageView
        android:id="@+id/imageViewStar5"
        android:layout_width="32dp"
        android:layout_height="22dp"
        android:contentDescription="reviewStar"
        app:layout_constraintStart_toEndOf="@+id/imageViewStar1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/starnull" />

    <ImageView
        android:id="@+id/imageViewCamera"
        android:layout_width="57dp"
        android:layout_height="41dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/imageViewStar3"
        app:srcCompat="@drawable/camera"
        tools:layout_editor_absoluteX="164dp" />


    <ImageView
        android:id="@+id/imageViewShelter"
        android:layout_width="69dp"
        android:layout_height="45dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="13dp"
        app:layout_constraintEnd_toStartOf="@+id/imageViewCamera"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/shelter2"
        tools:layout_editor_absoluteY="27dp" />


    <ImageView
        android:id="@+id/imageView24hrs"
        android:layout_width="57dp"
        android:layout_height="41dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewCamera"
        app:srcCompat="@drawable/allday"
        tools:layout_editor_absoluteY="30dp" />


</android.support.constraint.ConstraintLayout>